@model RestAPICSharp.Models.PboRequest
@{
    ViewBag.Title = "Token";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
   
    <div class="clearfix">&nbsp;</div>
    <div class="row">
        <div class="col-md-12">
            @Html.ValidationSummary(false)
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
          <h5><b>Elibility Inquiry -  Dependent</b></h5>
        </div>
    </div>
    <div class="row">
        <div class="col-md-2">
            <h5><b>Method</b></h5>
        </div>
        <div class="col-md-2">
            <h5><b>POST</b></h5>
        </div>
        <div class="col-md-4">

        </div>
        <div class="col-md-4">

        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
           <h5><b>Request Headers</b></h5> 
        </div>
    </div>
    <div class="clearfix">&nbsp;</div>
<div class="row">
    <div class="col-md-2">
        Authorization
    </div>
    <div class="col-md-1">
     Bearer [token]
       
    </div>
    <div class="col-md-6">
        @Html.TextBoxFor(m => m.Response.Token, "", new { @class = "form-control" })
    </div>
    <div class="col-md-3">
    </div>
    </div>
    <div class="clearfix">&nbsp;</div>
    <div class="row">
        <div class="col-md-2">
          Client-User-Name
        </div>
        <div class="col-md-4">
            @Html.TextBoxFor(m => m.Response.ClientUserName, "", new { @class = "form-control" })

        </div>
       
        <div class="col-md-6">
        </div>
    </div>
    <div class="clearfix">&nbsp;</div>
    <div class="row">
        <div class="col-md-2">
            Client-Password
        </div>
        <div class="col-md-4">
            @Html.PasswordFor(m => m.Response.ClientPassword, new { @class = "form-control" })

        </div>
        
        <div class="col-md-6">
        </div>
    </div>
    <div class="clearfix">&nbsp;</div>
    <div class="row">
        <div class="col-md-12">
          <h5><b> Request Body-Parameters </b></h5>
        </div>
     </div>
  <div class="clearfix">&nbsp;</div>


    <div class="row">
        <div class="col-md-3">
            Payer Code <span style="color:red">*</span>
            @Html.TextBoxFor(m => m.Request.PayerCode, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
            Payer Name
            @Html.TextBoxFor(m => m.Request.PayerName, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
          
        </div>
        <div class="col-md-3">
            </div>
    </div>
    <div class="clearfix">&nbsp;</div>
    <div class="row">
        <div class="col-md-3">
          Provider NPI <span style="color:red">*</span>
            @Html.TextBoxFor(m => m.Request.Provider.NPI, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
            Provider FirstName
            @Html.TextBoxFor(m => m.Request.Provider.FirstName, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
            Provider Middle Name
            @Html.TextBoxFor(m => m.Request.Provider.MiddleName, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
            Provider Last Name <span style="color:red">*</span>
            @Html.TextBoxFor(m => m.Request.Provider.LastName, "", new { @class = "form-control" })
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            Provider Group NPI
            @Html.TextBoxFor(m => m.Request.Provider.GroupNPI, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
            Provider PIN
            @Html.TextBoxFor(m => m.Request.Provider.PIN, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
            Provider Taxanomy
            @Html.TextBoxFor(m => m.Request.Provider.Taxonomy, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
            Provider Federal Tax ID
            @Html.TextBoxFor(m => m.Request.Provider.FederalTaxID, "", new { @class = "form-control" })
        </div>
    </div>
    <div class="clearfix">&nbsp;</div>
    <div class="row">
        <div class="col-md-3">
          Member Id <span style="color:red">*</span>
            @Html.TextBoxFor(m => m.Request.Subscriber.MemberID, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
           Dependent First Name <span style="color:red">*</span>
            @Html.TextBoxFor(m => m.Request.Dependent.Patient.FirstName, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
            Dependent Middle Name
            @Html.TextBoxFor(m => m.Request.Dependent.Patient.MiddleName, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
            Dependent Last Name <span style="color:red">*</span>
            @Html.TextBoxFor(m => m.Request.Dependent.Patient.LastName, "", new { @class = "form-control" })
        </div>
    </div>
    <div class="row">
        <div class="col-md-3">
            Dependent DOB(MM/dd/yyyy) <span style="color:red">*</span>
            @Html.TextBoxFor(m => m.Request.Dependent.Patient.DOB, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
            Dependent Gender
            @Html.TextBoxFor(m => m.Request.Dependent.Patient.Gender, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
            Relation With Subscriber
            @Html.TextBoxFor(m => m.Request.Dependent.RelationWithSubscriber, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
         
        </div>
      
       
    </div>

    <div class="row">
      
        <div class="col-md-3">
            DOS Start Date (MM/dd/yyyy) <span style="color:red">*</span>
            @Html.TextBoxFor(m => m.Request.DOS_StartDate, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
            DOS End Date (MM/dd/yyyy) <span style="color:red">*</span>
            @Html.TextBoxFor(m => m.Request.DOS_EndDate, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
         Service codes (30,58,56)
            @Html.TextBoxFor(m => m.ServiceCodes, "", new { @class = "form-control" })
        </div>
        <div class="col-md-3">
         
        </div>
    </div>
    <div class="clearfix">&nbsp;</div>
    <div class="row">
        <div class="col-md-2">

        </div>
        <div class="col-md-4">
            <input type="submit" value="Run Inquiry" class="btn btn-default" />
        </div>
        <div class="col-md-6">

        </div>
    </div>
    <div class="clearfix">&nbsp;</div>
    <div class="row">
        <div class="col-md-2">
        <b> API Json Request</b>
        </div>
        <div class="col-md-8">

           @Html.TextArea("ApiRequest",Model.Response.ApiRequest??"" ,new { style = "border-style:Solid;font-family:Courier New;font-size:8pt;height:400px;background-color: white;max-width:700px", cols = 80, rows = 15 })
        </div>

        <div class="col-md-2">
            @*<button onclick="prettyPrint()">Pretty Print</button>*@
        </div>

        <div id="divRequest" style="display:none">
            @Model.Response.ApiRequest
        </div>
    </div>
        <div class="row">
            <div class="col-md-12">
                <h3><b>API Response</b></h3>
            </div>
        </div>
        <div class="clearfix">&nbsp;</div>
        <div class="row">
            <div class="col-md-2">
                Status Code
            </div>
            <div class="col-md-1">
                <b>    @( (int)Model.Response.StatusCode) </b>
            </div>
            <div class="col-md-9">
                <b>   @Model.Response.StatusCode.ToString()</b>
            </div>

        </div>
        <div class="clearfix">&nbsp;</div>
        <div class="row">
            <div class="col-md-2">
                Response
            </div>
            <div class="col-md-8">


                @Html.TextArea("ApiResponse",Model.Response.ApiResponse??"", new { style = "border-style:Solid;font-family:Courier New;font-size:8pt;height:400px;background-color: white;max-width:700px", cols = 120, rows = 15 })
            </div>

            <div class="col-md-2">
                @*<button onclick="prettyPrint()">Pretty Print</button>*@
            </div>
            <div id="divRes" style="display:none">
                @Model.Response.ApiResponse
            </div>
        </div>
        }

